FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    
    walls
    {
        type            zeroGradient;
    }
    
    top
    {
        type            zeroGradient;
    }

    bottom
    {
        type            zeroGradient;
    }
    
    defaultFaces
    {
        type            empty;
    }

    obstacle
    {
        type            zeroGradient;
    }

    inlet
    {
        type            uniformFixedValue;

        uniformValue
        {
            type        constant;
            value       1.0;
        }
    }
    
    outlet
    {
        type            zeroGradient;
    }
}


