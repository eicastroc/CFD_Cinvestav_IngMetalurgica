FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "20";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.375;

boundaryField
{
    walls
    {
        type            kqRWallFunction;
        value           uniform 0.375;
    }
    top
    {
        type            kqRWallFunction;
        value           uniform 0.375;
    }
    bottom
    {
        type            kqRWallFunction;
        value           uniform 0.375;
    }
    frontAndBack
    {
        type            empty;
    }
    inlet
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.01;
        value           uniform 0.375;
    }
    outlet
    {
        type            zeroGradient;
    }
}


